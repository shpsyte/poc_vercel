  "scripts": {
    "dev": "yarn generate-config && npm-run-all --parallel dev:*",
    "dev:server": "browser-refresh ./build/bundle.js",
    "dev:build-server": "webpack --mode development --config webpack.server.js --watch",
    "dev:build-client": "webpack --mode development --config webpack.client.js --watch",
    "build": "yarn clean && yarn generate-config && npm-run-all --parallel build:*",
    "clean": "rm -rf ./public ./build",
    "build:client": "webpack -p --config webpack.client-prod.js",
    "build:server": "webpack -p --config webpack.server.js ",
    "server": "node --max-http-header-size=81000 --max-old-space-size=4096 public/build/bundle.js",
    "start": "node --max-http-header-size=81000 --max-old-space-size=4096 public/build/bundle.js",
    "generate-config": "node ./helpers/generate-config.js"
